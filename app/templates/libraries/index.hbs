<h2>List</h2>
<div class="row">
  {{#each model as |library|}}
  <div class="col-md-4">
    {{#library-item item=library}}
    {{#link-to 'libraries.edit' library.id class='btn btn-warning btn-sm'}}Edit{{/link-to}}
    <button class="btn btn-danger btn-sm" {{action 'deleteLibrary' library}}>Delete</button>
    {{/library-item}}
  </div>
  {{/each}}
</div>